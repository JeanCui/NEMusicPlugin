{
  "manifest_version": 2,
  "name": "Unblock NetEase Music",
  "description": "This extension would unblock netease music website!", 
  "version": "1.0",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "proxy",
    "http://cn-proxy.com/"
  ]
}
